Players = game:GetService('Players')

EgoId = {
    [0] = true,
}

fedId = {
    [0] = true,
}

ThugId = {
    [0] = true,
    [1] = true,
    [2] = true,
}

StarId = {
    [7191409064] = true,
    [7327961600] = true,
}

SkidId = {
    [] = true,
}

OwnerId = {
    [7360048981] = true,
    [7361303723] = true,
}

EnemyId = {
    [0] = true,
}

success, err = pcall(function()
    for _, player in pairs(Players:GetChildren()) do
        userId = player.UserId
        if StarId[userId] then
            if player.Character and player.Character.Parent.Name == 'Players' then
                humanoid = player.Character:FindFirstChildWhichIsA('Humanoid')
                if humanoid then
                    humanoid.DisplayName = '[‚≠ê] ' .. player.DisplayName
                end
            end
        elseif SkidId[userId] then
            if player.Character and player.Character.Parent.Name == 'Players' then
                humanoid = player.Character:FindFirstChildWhichIsA('Humanoid')
                if humanoid then
                    humanoid.DisplayName = '[ü•∑] ' .. player.DisplayName
                end
            end
        elseif ThugId[userId] then
            if player.Character and player.Character.Parent.Name == 'Players' then
                humanoid = player.Character:FindFirstChildWhichIsA('Humanoid')
                if humanoid then
                    humanoid.DisplayName = '[üõ°Ô∏è] ' .. player.DisplayName
                end
            end
        elseif EgoId[userId] or fedId[userId] then
            if player.Character and player.Character.Parent.Name == 'Players' then
                humanoid = player.Character:FindFirstChildWhichIsA('Humanoid')
                if humanoid then
                    humanoid.DisplayName = 'null'
                end
            end
        elseif OwnerId[userId] then
            if player.Character and player.Character.Parent.Name == 'Players' then
                humanoid = player.Character:FindFirstChildWhichIsA('Humanoid')
                if humanoid then
                    humanoid.DisplayName = '[üëë] ' .. player.DisplayName
                end
            end
        elseif EnemyId[userId] then
            if player.Character and player.Character.Parent.Name == 'Players' then
                humanoid = player.Character:FindFirstChildWhichIsA('Humanoid')
                if humanoid then
                    humanoid.DisplayName = 'null'
                end
            end
        else
            if player.Character and player.Character.Parent.Name == 'Players' then
                humanoid = player.Character:FindFirstChildWhichIsA('Humanoid')
                if humanoid and not player.Character:FindFirstChild('UpperTorso'):FindFirstChild('BodyBackAttachment') then
                    humanoid.DisplayName = '[üòé] ' .. player.DisplayName
                end
            end
        end
    end
end)
